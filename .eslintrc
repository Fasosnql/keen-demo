{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "import"
  ],
  "extends": ["plugin:react/recommended"],
  "env": {
    "browser": true,
    "jest": true
  },
  "settings": {
    "import/parser": "babel-eslint",
    "import/resolver": {
      "babel-module": {}
    }
  },
  "rules": {
    "linebreak-style": 0,
    "getter-return": 2,
    "no-compare-neg-zero": 2,
    "no-cond-assign": 2,
    "no-console": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": [
      "error", "all",
      {
        "ignoreJSX": "all",
        "nestedBinaryExpressions": false
      }
    ],
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 0,
    "no-obj-calls": 2,
    "no-prototype-builtins": 0,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-template-curly-in-string": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "use-isnan": 2,
    "valid-jsdoc": 0,
    "valid-typeof": 2,
    "accessor-pairs": 0,
    "array-callback-return": 2,
    "block-scoped-var": 2,
    "class-methods-use-this": 0,
    "complexity": ["error", 10],
    "consistent-return": 2,
    "curly": 2,
    "default-case": 1,
    "dot-location": ["error", "property"],
    "dot-notation": [
      "error", {
        "allowKeywords": false
      }
    ],
    "eqeqeq": ["error", "always"],
    "guard-for-in": 0,
    "no-alert": 2,
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-div-regex": 0,
    "no-else-return": 2,
    "no-empty-function": 0,
    "no-empty-pattern": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-label": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-global-assign": [
      "error", {
        "exceptions": ["Object"]
      }
    ],
    "no-implicit-coercion": 0,
    "no-implicit-globals": 0,
    "no-implied-eval": 2,
    "no-invalid-this": 0,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-magic-numbers": 0,
    "no-multi-spaces": 2,
    "no-multi-str": 0,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-restricted-properties": 0,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unmodified-loop-condition": 2,
    "no-unused-expressions": 2,
    "no-unused-labels": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-useless-escape": 2,
    "no-useless-return": 2,
    "no-void": 1,
    "no-warning-comments": 0,
    "no-with": 2,
    "prefer-promise-reject-errors": 0,
    "radix": 0,
    "require-await": 2,
    "vars-on-top": 1,
    "wrap-iife": 2,
    "yoda": 2,
    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-restricted-globals": 2,
    "no-shadow": [
      "error", {
        "hoist": "functions"
      }
    ],
    "no-undef": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 0,
    "array-bracket-spacing": ["error", "never"],
    "brace-style": 2,
    "camelcase": 0,
    "comma-dangle": ["error", "always-multiline"],
    "comma-spacing": 2,
    "comma-style": 2,
    "computed-property-spacing": 2,
    "eol-last": ["error", "always"],
    "func-call-spacing": 2,
    "jsx-quotes": ["error", "prefer-double"],
    "keyword-spacing": 2,
    "max-depth": ["error", 4],
    "max-len": ["error", 120],
    "max-nested-callbacks": ["error", 4],
    "max-params": ["error", 5],
    "max-statements": ["error", 25],
    "max-statements-per-line": [
      "error", {
        "max": 1
      }
    ],
    "no-bitwise": 2,
    "no-continue": 2,
    "no-lonely-if": 2,
    "no-mixed-operators": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-assign": 2,
    "no-multiple-empty-lines": [
      "error", {
        "max": 1
      }
    ],
    "no-negated-condition": 0,
    "no-new-object": 2,
    "no-plusplus": 2,
    "no-tabs": 2,
    "no-trailing-spaces": 2,
    "no-unneeded-ternary": 2,
    "no-whitespace-before-property": 2,
    "object-curly-newline": 0,
    "object-curly-spacing": 0,
    "object-property-newline": 2,
    "one-var": ["error", "never"],
    "operator-assignment": 2,
    "operator-linebreak": 0,
    "quote-props": ["error", "consistent"],
    "quotes": ["error", "single"],
    "require-jsdoc": [
      "error", {
        "require": {
          "FunctionDeclaration": false,
          "MethodDefinition": false,
          "ClassDeclaration": true,
          "ArrowFunctionExpression": false
        }
      }
    ],
    "semi": 2,
    "semi-style": 2,
    "sort-keys": 0,
    "space-before-blocks": 2,
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "spaced-comment": 2,
    "switch-colon-spacing": 2,
    "arrow-body-style": ["error", "as-needed"],
    "arrow-parens": ["error", "always"],
    "arrow-spacing": 2,
    "constructor-super": 2,
    "no-class-assign": 2,
    "no-confusing-arrow": 2,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-duplicate-imports": 2,
    "no-new-symbol": 2,
    "no-this-before-super": 2,
    "no-useless-computed-key": 2,
    "no-useless-constructor": 2,
    "no-useless-rename": 2,
    "no-var": 2,
    "object-shorthand": 2,
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-template": 2,
    "react/default-props-match-prop-types": ["error", {"allowRequiredDefaults": true}]
  }
}
